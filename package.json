{
  "name": "mine-h5-ui",
  "version": "1.2.0",
  "private": false,
  "author": "biaov<biaov@qq.com>",
  "main": "lib/mine-h5-ui.umd.min.js",
  "style": "lib/theme-default/index.css",
  "scripts": {
    "start": "npm run serve",
    "serve": "vue-cli-service serve",
    "build": "npm run build:docs && npm run build:lib && npm run build:theme",
    "build:docs": "vue-cli-service build",
    "build:lib": "vue-cli-service build --target lib --name mine-h5-ui --dest lib packages/index.js",
    "build:theme": "gulp build -f packages/theme-default/gulpfile.js && cp-cli packages/theme-default/lib lib/theme-default",
    "build:theme-rem": "gulp build -f packages/theme-default/gulpfile-rem.js && cp-cli packages/theme-default/lib lib/theme-default",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "vue": "^2.6.11",
    "vue-clipboard2": "^0.3.1",
    "vue-router": "^3.1.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.0",
    "@vue/cli-plugin-eslint": "^4.3.0",
    "@vue/cli-plugin-unit-jest": "^4.3.1",
    "@vue/cli-service": "^4.3.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-eslint": "^10.1.0",
    "cp-cli": "^2.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "markdown-it-container": "^2.0.0",
    "style-resources-loader": "^1.3.3",
    "vue-markdown-loader": "^2.4.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [{
      "files": [
        "**/__tests__/*.{j,t}s?(x)",
        "**/tests/unit/**/*.spec.{j,t}s?(x)"
      ],
      "env": {
        "jest": true
      }
    }]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest",
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/packages/$1"
    }
  },
  "keywords": [
    "MINE-H5-UI",
    "Vue",
    "VueJs",
    "UI组件"
  ],
  "publishConfig": { "registry": "https://npm.pkg.github.com/" },
  "repository": {
    "type": "git",
    "url": "https://github.com/biaov/MINE-H5-UI.git"
  },
  "license": "ISC"
}